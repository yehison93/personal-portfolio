---
import Social from "./Social.astro";
const socialLink = {
  twitter: "yehison93",
  gitHub: "yehison93",
  youtube: "yehison93",
  instagram: "yehison93",
};
---

<style>
  footer {
    display: flex;
    gap: 1rem;
    margin-top: 2rem;
  }
</style>

<footer>
